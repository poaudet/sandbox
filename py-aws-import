import boto3
import json
import os
from decimal import Decimal

dynamodb = boto3.resource('dynamodb')
table = dynamodb.Table('pfa_dev')
directory = '/home/poaudet/AWS/toImport/'
json_string = '{"key1": 1, "key2": "value"}'


for filename in os.listdir(directory):
    print(filename)
    fPath = os.path.join(directory, filename)
    with open(fPath) as f:
      contents = f.read()
      json_obj = json.loads(contents, parse_float=Decimal) 
      table.put_item(Item=json_obj)